{
  "$schema": "https://raw.githubusercontent.com/dotnet/Nerdbank.GitVersioning/main/src/NerdBank.GitVersioning/version.schema.json",
  "version": "104.8",
  "assemblyVersion": {
    "precision": "minor"
  },
  "nugetPackageVersion": {
    "semVer": 2
  },
  "publicReleaseRefSpec": [
    "^refs/heads/master$",
    "^refs/heads/main$",
    "^refs/heads/v\\d+(?:\\.\\d+)?$",
    "^refs/heads/v\\d+\\.\\d+$",
    "^refs/tags/v\\d+\\.\\d+$"
  ],
  "cloudBuild": {
    "buildNumber": {
      "enabled": true
    }
  }
}